<div class="container">
  <div class="row">
    <div class="col-md-8 mx-auto">
      <h2>REGISTRATE</h2>
      <br />
      <form>
        <div class="mb-3">
          <label class="form-label">Nombre:</label>
          <input
            type="text"
            class="form-control"
            id="first_name"
            name="first_name"
            required
          />
          <br /></div>
        <div class="mb-3">
          <label class="form-label">Apellido:</label>
          <input
            type="text"
            class="form-control"
            id="last_name"
            name="last_name"
            required
          />
          <br />
        </div>
        <div class="mb-3">
          <label class="form-label">Email:</label>
          <input
            type="text"
            class="form-control"
            id="email"
            name="email"
            required
          />
          <br /></div>
        <div class="mb-3">
          <label class="form-label">Edad:</label>
          <input
            type="text"
            class="form-control"
            id="age"
            name="age"
            required
          />
          <br /></div>
        <div class="mb-3">
          <label class="form-label">Contrase√±a:</label>
          <input
            type="text"
            class="form-control"
            id="password"
            name="password"
            required
          />
          <br /></div>
        <button
          type="button"
          class="btn btn-light"
          id="btnRegister"
        >Enviar</button>
        <p> Si ya estas registrado <a href="/users/login"> Ingrese aqui </a></p>
      </form>
    </div>
  </div>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="js/register.js"></script>