<main class="min-w-full bg-[#fdf6f6] mt-[70px]">
  <h1 class="font-bold">Cart ID: {{cart.id}}</h1>
  <div class="container">
    {{#each cart.products}}
      <div class="row mb-4">
        <div class="col-3">
          <img
            src="{{thumbnail}}"
            alt="imagen del producto"
            class="img-fluid"
          />
        </div>
        <div class="col-3">
          <p>Nombre: {{this.getProducts.title}}</p>
        </div>
        <div class="col-3">
          <p>Precio: {{this.price}}</p>
        </div>
        <div class="col-3">
          <p>Cantidad: {{quantity}}</p>
        </div>
      </div>
    {{/each}}
  </div>
</main>